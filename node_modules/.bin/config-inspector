#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/dingming/Documents/GitHub/academicpage/node_modules/.pnpm/@eslint+config-inspector@0.5.6_eslint@9.16.0_jiti@2.4.1_supports-color@8.1.1__supports-color@8.1.1/node_modules/@eslint/config-inspector/node_modules:/Users/dingming/Documents/GitHub/academicpage/node_modules/.pnpm/@eslint+config-inspector@0.5.6_eslint@9.16.0_jiti@2.4.1_supports-color@8.1.1__supports-color@8.1.1/node_modules/@eslint/node_modules:/Users/dingming/Documents/GitHub/academicpage/node_modules/.pnpm/@eslint+config-inspector@0.5.6_eslint@9.16.0_jiti@2.4.1_supports-color@8.1.1__supports-color@8.1.1/node_modules:/Users/dingming/Documents/GitHub/academicpage/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/dingming/Documents/GitHub/academicpage/node_modules/.pnpm/@eslint+config-inspector@0.5.6_eslint@9.16.0_jiti@2.4.1_supports-color@8.1.1__supports-color@8.1.1/node_modules/@eslint/config-inspector/node_modules:/Users/dingming/Documents/GitHub/academicpage/node_modules/.pnpm/@eslint+config-inspector@0.5.6_eslint@9.16.0_jiti@2.4.1_supports-color@8.1.1__supports-color@8.1.1/node_modules/@eslint/node_modules:/Users/dingming/Documents/GitHub/academicpage/node_modules/.pnpm/@eslint+config-inspector@0.5.6_eslint@9.16.0_jiti@2.4.1_supports-color@8.1.1__supports-color@8.1.1/node_modules:/Users/dingming/Documents/GitHub/academicpage/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@eslint/config-inspector/bin.mjs" "$@"
else
  exec node  "$basedir/../@eslint/config-inspector/bin.mjs" "$@"
fi
