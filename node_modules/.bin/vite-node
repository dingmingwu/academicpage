#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/dingming/Documents/GitHub/academicpage/node_modules/.pnpm/vite-node@2.1.8_@types+node@22.10.1_sass@1.82.0_supports-color@8.1.1_terser@5.37.0/node_modules/vite-node/node_modules:/Users/dingming/Documents/GitHub/academicpage/node_modules/.pnpm/vite-node@2.1.8_@types+node@22.10.1_sass@1.82.0_supports-color@8.1.1_terser@5.37.0/node_modules:/Users/dingming/Documents/GitHub/academicpage/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/dingming/Documents/GitHub/academicpage/node_modules/.pnpm/vite-node@2.1.8_@types+node@22.10.1_sass@1.82.0_supports-color@8.1.1_terser@5.37.0/node_modules/vite-node/node_modules:/Users/dingming/Documents/GitHub/academicpage/node_modules/.pnpm/vite-node@2.1.8_@types+node@22.10.1_sass@1.82.0_supports-color@8.1.1_terser@5.37.0/node_modules:/Users/dingming/Documents/GitHub/academicpage/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite-node/vite-node.mjs" "$@"
else
  exec node  "$basedir/../vite-node/vite-node.mjs" "$@"
fi
