#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/dingming/Documents/GitHub/academicpage/node_modules/.pnpm/@nuxt+devtools@1.6.3_rollup@4.28.1_supports-color@8.1.1_vite@5.4.11_@types+node@22.10.1_sass@_eeusbm3fvlmrfw2o4k55c33dvq/node_modules/@nuxt/devtools/node_modules:/Users/dingming/Documents/GitHub/academicpage/node_modules/.pnpm/@nuxt+devtools@1.6.3_rollup@4.28.1_supports-color@8.1.1_vite@5.4.11_@types+node@22.10.1_sass@_eeusbm3fvlmrfw2o4k55c33dvq/node_modules/@nuxt/node_modules:/Users/dingming/Documents/GitHub/academicpage/node_modules/.pnpm/@nuxt+devtools@1.6.3_rollup@4.28.1_supports-color@8.1.1_vite@5.4.11_@types+node@22.10.1_sass@_eeusbm3fvlmrfw2o4k55c33dvq/node_modules:/Users/dingming/Documents/GitHub/academicpage/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/dingming/Documents/GitHub/academicpage/node_modules/.pnpm/@nuxt+devtools@1.6.3_rollup@4.28.1_supports-color@8.1.1_vite@5.4.11_@types+node@22.10.1_sass@_eeusbm3fvlmrfw2o4k55c33dvq/node_modules/@nuxt/devtools/node_modules:/Users/dingming/Documents/GitHub/academicpage/node_modules/.pnpm/@nuxt+devtools@1.6.3_rollup@4.28.1_supports-color@8.1.1_vite@5.4.11_@types+node@22.10.1_sass@_eeusbm3fvlmrfw2o4k55c33dvq/node_modules/@nuxt/node_modules:/Users/dingming/Documents/GitHub/academicpage/node_modules/.pnpm/@nuxt+devtools@1.6.3_rollup@4.28.1_supports-color@8.1.1_vite@5.4.11_@types+node@22.10.1_sass@_eeusbm3fvlmrfw2o4k55c33dvq/node_modules:/Users/dingming/Documents/GitHub/academicpage/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@nuxt/devtools/cli.mjs" "$@"
else
  exec node  "$basedir/../@nuxt/devtools/cli.mjs" "$@"
fi
